#include <iostream>
#include <vector>
#include <algorithm>

int main() {
    int M, N;
    std::cout << "Введіть розмір масиву A (M): ";
    std::cin >> M;
    std::cout << "Введіть розмір масиву B (N): ";
    std::cin >> N;
    
    std::vector<int> A(M), B(N);
    
    std::cout << "Введіть елементи масиву A:" << std::endl;
    for (int i = 0; i < M; ++i) {
        std::cin >> A[i];
    }
    
    std::cout << "Введіть елементи масиву B:" << std::endl;
    for (int i = 0; i < N; ++i) {
        std::cin >> B[i];
    }
    
    std::vector<int> C;
    

    for (int i = 0; i < M; ++i) {
        if (std::find(B.begin(), B.end(), A[i]) == B.end() && std::find(C.begin(), C.end(), A[i]) == C.end()) {
            C.push_back(A[i]);
        }
    }
    
    
    for (int i = 0; i < N; ++i) {
        if (std::find(A.begin(), A.end(), B[i]) == A.end() && std::find(C.begin(), C.end(), B[i]) == C.end()) {
            C.push_back(B[i]);
        }
    }
    
    std::cout << "Результат утвореного масиву C:" << std::endl;
    for (int num : C) {
        std::cout << num << " ";
    }
    std::cout << std::endl;
    
    return 0;
}
